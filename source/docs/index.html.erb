---
layout: layout
---
<header class="artwork"></header>

<article>
  <h1>Documentation</h1>

  <nav>
    <ul>
      <% current_resource.children.each do |resource| %>
        <li>
          <h2><%= link_to resource.data.title, resource.url %></h2>
          <ul>
            <% resource.children.each do |child| %>
              <li>
                <%=  link_to child.data.title, child.url %>
              </li>
            <% end %>
          </ul>
        </li>
      <% end %>
    </ul>
  </nav>
</article>
